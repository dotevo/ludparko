  "repository": {
    "type": "git",
    "url": "https://github.com/dotevo/ludparko.git"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.9.1",
    "@parcel/transformer-typescript-tsc": "^2.9.1",
    "@parcel/transformer-typescript-types": "^2.9.1",
    "gh-pages": "^5.0.0",
    "parcel": "^2.9.1",
    "prettier": "^2.8.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.4"
  }
