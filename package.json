{
  "name": "ludparko",
  "version": "0.0.1",
  "description": "OpenStreetMap playground map",
  "scripts": {
    "build": "parcel build src/index.ts --public-url ./ ",
    "test": "jest",
    "check": "tsc --noEmit",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "tslint -p tsconfig.json"
  },
